#mGroupBox > #Item {
    width: 60px;
    height: 70px;
    border-width: 2px;
    border-color: rgb(153, 153, 153);
    margin: 5px;
    align-items: center;
}

#mGroupBox > #Item > #Icon {
    width: 50px;
    height: 50px;
    border-width: 1px;
    border-color: antiquewhite;
}

#mGroupBox > #Item > #Name {
    width: 100%;
    -unity-text-align: upper-center;
}

#mGroupBox > #Item:hover {
    background-color: rgba(168, 168, 168, 0.24);
}
